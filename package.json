    "image-size": "^1.1.1",
    "isomorphic-dompurify": "^2.12.0",
    "next": "14.2.4",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@types/dompurify": "^3.0.5",
    "@types/node": "20.14.9",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "eslint": "8.57.0",
    "eslint-config-next": "14.2.4",
    "prettier": "^3.3.2",
    "typescript": "5.5.3"
  }
}
