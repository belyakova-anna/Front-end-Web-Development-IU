{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "outDir": "./dist",
      "esModuleInterop": true,
      "skipLibCheck": true,
      "forceConsistentCasingInFileNames": true
    },
    "include": ["src/**/*"]
  }
  